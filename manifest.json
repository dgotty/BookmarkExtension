{
  "manifest_version": 2,
  "name": "Bookmarks Manager",
  "options_page": "options.html",
  "description": "This extension helps you manage your bookmarks",
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "bookmarks"
  ]
}